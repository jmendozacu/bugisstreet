<?xml version="1.0" encoding="UTF-8"?>
<config>
    <sections>
        <carriers>
            <groups>
                <webkulshipping>
                    <label>Webkul Marketplace Table Rate Shipping Detail</label>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <sort_order>2</sort_order>
                    <fields>
                        <active translate="label">
						  <label>Enabled</label>
						  <frontend_type>select</frontend_type>
						  <source_model>adminhtml/system_config_source_yesno</source_model>
						  <sort_order>0</sort_order>
						  <show_in_default>1</show_in_default>
						  <show_in_website>1</show_in_website>
						  <show_in_store>1</show_in_store>
						</active>
						<title translate="label">
						  <label>Title</label>
						  <frontend_type>text</frontend_type>
						  <sort_order>2</sort_order>
						  <show_in_default>1</show_in_default>
						  <show_in_website>1</show_in_website>
						  <show_in_store>1</show_in_store>
						  <comment>e.g. Table Rate </comment>
						</title>
						<name translate="label">
						  <label>Name</label>
						  <frontend_type>text</frontend_type>
						  <sort_order>1</sort_order>
						  <show_in_default>1</show_in_default>
						  <show_in_website>1</show_in_website>
						  <show_in_store>1</show_in_store>
						  <comment>e.g. Webkul Table Rate Shipping</comment>
						</name>
						<sallowspecific translate="label">
						  <label>Ship to applicable countries</label>
						  <frontend_type>select</frontend_type>
						  <sort_order>4</sort_order>
						  <frontend_class>shipping-applicable-country</frontend_class>
						  <source_model>adminhtml/system_config_source_shipping_allspecificcountries
						  </source_model>
						  <show_in_default>2</show_in_default>
						  <show_in_website>1</show_in_website>
						  <show_in_store>1</show_in_store>
						</sallowspecific>
						<specificerrmsg translate="label">
						  <label>Displayed Error Message</label>
						  <frontend_type>textarea</frontend_type>
						  <sort_order>3</sort_order>
						  <show_in_default>1</show_in_default>
						  <show_in_website>1</show_in_website>
						  <show_in_store>1</show_in_store>
						</specificerrmsg>
                    </fields>
                </webkulshipping>
            </groups>
        </carriers>
    </sections>
</config>
